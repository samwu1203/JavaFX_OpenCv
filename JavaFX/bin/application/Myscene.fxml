<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MyController">
   <bottom>
      <HBox alignment="center">
         <padding>
            <Insets bottom="25" left="25" right="25" top="25" />
         </padding>
         <children>
            <Button fx:id="playvideo" alignment="center" onAction="#playVideo" text="Start" />
         </children>
      </HBox>
   </bottom>
   <center>
      <ImageView id="currentview" fx:id="currentview" fitHeight="527.0" fitWidth="672.0" pickOnBounds="true" smooth="false" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
